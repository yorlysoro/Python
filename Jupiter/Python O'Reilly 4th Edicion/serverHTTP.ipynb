    "port = 80\n",
    "\n",
    "os.chdir(webdir)\n",
    "srvraddr = (\"\", port)\n",
    "srvrobj = HTTPServer(srvraddr, CGIHTTPRequestHandler)\n",
    "srvrobj.serve_forever()"
